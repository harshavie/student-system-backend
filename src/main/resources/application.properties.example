# Application name
spring.application.name=studentsystem

# Database URL - Replace with your actual DB connection URL
spring.datasource.url=jdbc:mysql://localhost:3306/your_database_name

# Database username - Replace with your actual DB username
spring.datasource.username=your_db_username

# Database password - Replace with your actual DB password
spring.datasource.password=your_db_password

# Hibernate configuration
spring.jpa.hibernate.ddl-auto=update

# Database Driver
spring.datasource.driverClassName=com.mysql.cj.jdbc.Driver
